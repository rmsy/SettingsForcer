<PreferenceScreen xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <SwitchPreference
        android:defaultValue="false"
        android:key="autostart_service"
        android:summary="@string/pref_description_autostart_service"
        android:title="@string/pref_title_autostart_service"/>

    <ListPreference
        android:entries="@array/pref_setup_wizard_complete_titles"
        android:entryValues="@array/pref_setup_wizard_complete_values"
        android:key="setup_wizard_complete"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="@string/pref_title_setup_wizard_complete"/>

</PreferenceScreen>
